<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.joshua.joshflickr" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/travelico"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LocationsActivity"
            android:label="@string/title_activity_locations" >
        </activity>
        <activity
            android:name=".SpecificLocationActivity"
            android:label="@string/title_activity_specific_location" >
        </activity>
        <activity
            android:name=".SinglePhoto"
            android:label="@string/title_activity_single_photo" >
        </activity>
        <activity
            android:name=".AndroidSearchWOEIDActivity"
            android:label="@string/title_activity_android_search_woeid" >
        </activity>
    </application>

</manifest>
